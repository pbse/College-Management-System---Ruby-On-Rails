<div id="content-header">
<%= show_header_icon %>
  <h1><%= t('asset_liability_Management') %></h1>
<div class='header-sep'>|</div>
<div class='sub-header'><%= t('liability_form') %></div>
</div>
<div id="page-yield">

  <div class="link-heading">
    <%= link_to_function("#{t('create_text')}") { |page|
      page.replace_html 'modal-box', :partial => 'create_liability_form'
      page << "Modalbox.show($('modal-box'), {title: '#{t('creates_new_liability')}', width: 500});"
    } %>
  </div>
  <div class="link-descr"></div>
</div>
